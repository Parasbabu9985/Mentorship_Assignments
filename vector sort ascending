include <bits/stdc++.h>
using namespace std;
 

int f(int n) //function declaration
{
    
    if (n<=1) //condition
        return 1;//1 return1
        //2 condition fauil
        //3condition fail
 
    
    else //recurcive condition
    //3=3*3-1=int64_tfact of 3!
        return n * f(n - 1);
}
 

int main() //driver code
{
    int n ;
    cin>>n;
    cout<<"factorial of "<<n<<" is: "<<f(n);
    return 0;
}
