#include<stdc++.h>
using namespace std;
class stack{
    int capacity;
    int*arr;
    int top;
    
};
public:
stack(int c){
    this->capcity=c;
    arr=new int[c];
    this->top=-1;

    
}
void(push(int(data))){
    if(this->top=this->capacity=-1){
        cout<<"overflow\n";
        return;
}
this->top++;
this->arr[this->top]=data;
}
int pop(){
    if(this->top=-1){
        cout<<"undereflow\n";
        return;
        this->top--;
    };
    int top(){
        if(this->top==-1){
            cout<<"underflow\n";
            return int min;
        }
        bool empty(){
            return this->top=-1;
        }
        int size(){
             return this->top+1;
            
        }
        bool is full(){
            return this->top=this->capacity=-1;
        }
    };
    int main(){
        stack.st(5);
        st.push(1);
        st.push(2);
        st.push(3);
        cout<<st.top<<"\n")
        st.push(4);
        st.push(5);
        cout<<st.top()<<"\n";
        st.push(6);
        return 0;
    }
        
    }
        }
             
            
        }
        }
        }
    }
    }
}
