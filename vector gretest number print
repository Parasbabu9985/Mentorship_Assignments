#include<bits/stdc++.h>
using namespace std;

int main ()
{
  int i;
  vector < int >v (6);

  for (i = 0; i < v.size (); i++)
    {
      cin >> v[i];
    }

  cout << "enter the  x";
  int x;
  cin >> x;


  int count = 0;
  for (i = 0; i < v.size (); i++)
    {
      if (v[i] > x)
	{
	  count++;
	}

    }
  return 0;
}
