
#include <iostream>

using namespace std;

int main() {

    int n,temp,array[50],i,j;
    cout<<"enter the number of element";
    cin>>n;
    for(i=0;i<n;i++);
    {
        cout<<" ";
        cin>>array[i];
    }
        cout<<"shorting array using buble short";
        for(i=0;i<(n-1);i++) 
        {
            for(j=0;j<n-i-1;j++) 
            {
                if (array[j]>array[j+1]) 
                {
                    temp=array[j];
                    array[j]=array[j+1];
                    array[j+1]=temp;
                }
            }
        
        }
    
    cout<<"asending order";
    for(i=0;i<n;i++)
    {
        cout<<array[i];
    }

    return 0;
}
